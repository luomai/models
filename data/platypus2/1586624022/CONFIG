# commit 06fa5514be9e9cd4a823ad53372c6f1c8ac444f2 (HEAD -> lg-cifar10, origin/lg-cifar10)
#    expand loop

train_cifar10() {
    local epochs=$1
    local np=$2
    local single_bs=$3

    kungfu_run $np \
        python3 \
        official/resnet/cifar10_main.py \
        $FLAGS \
        -bs $((np * single_bs)) \
        -ng $np \
        -te $epochs
}

measure train_cifar10 182 4 32
